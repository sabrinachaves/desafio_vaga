{
  "plugins": ["@typescript-eslint/eslint-plugin", "prettier", "eslint-plugin-import"],

  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "warn",
    "max-len": ["warn", 120]
  },
  "env": {
    "node": true
  },
  "ignorePatterns": ["/node_modules/"]
}
